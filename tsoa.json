{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "outputDirectory": ".",
    "disableStrictValidation": true,
    "specVersion": 3
  },
  "routes": {
    "routesDir": "./build",
    "middleware": "express",
    "iocModule": "./src/ioc/index.ts"
  },
  "ignore": [
    "**/node_modules/**"
  ]
}